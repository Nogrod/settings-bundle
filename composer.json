{
  "name": "jbtronics/settings-bundle",
  "type": "symfony-bundle",
  "license": "MIT",
  "description": "A symfony bundle to easily define and use user specific configuration",
  "keywords": ["settings", "config", "symfony", "symfony-bundle"],
  "autoload": {
    "psr-4": {
      "Jbtronics\\SettingsBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jbtronics\\SettingsBundle\\Tests\\TestApplication\\": "tests/TestApplication/src/",
      "Jbtronics\\SettingsBundle\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Jan BÃ¶hmer",
      "email": "mail@jan-boehmer.de"
    }
  ],
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "symfony/framework-bundle": "^6.4|^7.0",
    "symfony/translation-contracts": "^2.5|^3.0",
    "spatie/php-structure-discoverer": "^2.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "symfony/phpunit-bridge": "^6.4|^7.0",
    "phpunit/phpunit": "^9.5"
  }
}
